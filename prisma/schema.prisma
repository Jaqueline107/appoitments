// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Appointment {
  id       String   @id @unique @default(uuid())
  provider String
  date     DateTime
  where    String
}

model User {
  id       String @id @unique @default(uuid())
  email    String
  name     String
  password String
}

model Profile_Img {
  id   String @id @unique @default(uuid())
  name String
}
